<h2>Cities</h2>
<ol class="city">
  <% @list.each do |city| %>
    <%= link_to city.name, city_path(city.id), remote:true %>
  <% end %>
</ol>
<p><%= link_to "Back to Country Selection", countries_path(territory_id: Territory.find(Country.find(@list.first.country_id).territory_id)), remote:true %></p>
